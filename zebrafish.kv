#:kivy 2.0.0
<Button>:
    bold: True 
    color: (1, 1, 1, 1)
    background_color: (0.1, 0.1, 0.1, 1)
    font_size: 15
<TextInput>:
    color: (1, 1, 1, 1)
    font_size: 15
<Label>:
    background_color: (0.1, 0.1, 0.1, 1)
    canvas.before:
        Color:
            rgba: self.background_color
        Rectangle:
            size: self.size
            pos: self.pos
